<?xml version="1.0"?>
<!-- This file registers new types with portal_types. The types are
     then configured with the corresponding files in types/*.xml. Note
     that spaces are allowed in type names, but the corresponding XML file
     uses an underscore instead. The "Factory-based Type Information with
     dynamic views" refers to an FTI from Products.CMFDynamicViewFTI,
     which supports Plone's "display" menu.
  -->
<object name="portal_types" meta_type="Plone Types Tool">
  <!-- -*- extra stuff goes here -*- -->
  <object name="totav.stripe.invoice" meta_type="Dexterity FTI" />
  <object name="totav.stripe.subscription" meta_type="Dexterity FTI" />
  <object name="totav.stripe.card" meta_type="Dexterity FTI" />
  <object name="totav.stripe.coupon" meta_type="Dexterity FTI" />
  <object name="totav.stripe.plan" meta_type="Dexterity FTI" />
  <object name="totav.stripe.customer" meta_type="Dexterity FTI" />
  <object name="totav.stripe.domain" meta_type="Dexterity FTI" />
</object>
